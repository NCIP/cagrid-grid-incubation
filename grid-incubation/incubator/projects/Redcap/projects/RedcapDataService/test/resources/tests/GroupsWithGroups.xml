<gov:CQLQuery xmlns:gov="http://CQL.caBIG/1/gov.nih.nci.cagrid.CQLQuery">
	<gov:Target name="org.cagrid.redcap.Projects">
                  <gov:Group logicRelation="OR">
                     <gov:Group logicRelation="AND">
                        <gov:Attribute name="projectName" value="redcap%" predicate="LIKE"/>
                        <gov:Attribute name="projectId" value="7" predicate="EQUAL_TO"/>
                     </gov:Group>
                     <gov:Group logicRelation="AND">
                        <gov:Attribute name="appTitle" value="trial%" predicate="LIKE"/>
                        <gov:Attribute name="projectId" value="8" predicate="EQUAL_TO"/>
                     </gov:Group>
                     
                  </gov:Group>
               </gov:Target>
</gov:CQLQuery>