<DataServiceStyle xmlns="http://gov.nih.nci.cagrid.data/Style"
	name="I2B2" version="1.3">
	<CompatibleCaGridVersions>
		<Version>1.3</Version>
	</CompatibleCaGridVersions>
	<StyleDescription>Configures the data service to utilize a data source generated by the i2b2 Ontomapper</StyleDescription>
	<CreationWizardPanels>
		<CreationWizardPanel classname="org.cagrid.i2b2.ontomapper.style.wizard.IntroductionPanel" />
		<CreationWizardPanel classname="org.cagrid.i2b2.ontomapper.style.wizard.DomainModelPanel" />
	</CreationWizardPanels>
	<ModificationUiPanel
		classname="org.cagrid.i2b2.ontomapper.style.ModificationPanel" />
	<CreationHelper
		postCreationClassname="org.cagrid.i2b2.ontomapper.style.helper.I2B2PostCreationHelper" />
	<CodegenHelpers
		postCodegenClassname="org.cagrid.i2b2.ontomapper.style.helper.I2B2PostCodegenHelper"
		preCodegenClassname="org.cagrid.i2b2.ontomapper.style.helper.I2B2PreCodegenHelper" />
</DataServiceStyle>
