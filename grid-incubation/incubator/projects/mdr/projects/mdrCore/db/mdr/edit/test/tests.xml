<cgRegression:mdr-tests xmlns:cgRegression="http://www.cagrid.org/schema/cgRegression" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../regression-test/regression-tests.xsd">
    <cgRegression:mdr-test type="web-page-comparison">
        <cgRegression:test-name>supported object class listing</cgRegression:test-name>
        <cgRegression:test-desc>tests the object class listing page against expected output</cgRegression:test-desc>
