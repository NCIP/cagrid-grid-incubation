        <cgRegression:test-name>supported object class with parameter</cgRegression:test-name>
        <cgRegression:test-desc>tests the object class listing page with a parameter against expected output</cgRegression:test-desc>
        <cgRegression:script>supported_object_classes.xquery?letter=d</cgRegression:script>
        <cgRegression:comparison>test/object-class-listing01.xml</cgRegression:comparison>
        <cgRegression:module>edit</cgRegression:module>
    </cgRegression:mdr-test>
