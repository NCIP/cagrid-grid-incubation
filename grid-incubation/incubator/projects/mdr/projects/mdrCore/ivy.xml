<?xml version="1.0"?>

<ivy-module version="2.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="http://incubator.apache.org/ivy/schemas/ivy.xsd">

	<info organisation="caGrid" module="mdr" revision="${project.version}">
		<license name="caGrid" url="http://cagrid.org/wiki/CaGrid:LICENSE-1.3" />
		<description>MDR ISO 11179 Database and Application</description>
	</info>

	<configurations>
		<conf name="impl" visibility="private" />
		<conf name="stylesheets" visibility="public" />
		<conf name="default" visibility="public" extends="impl" />
	</configurations>

	<publications>
		<artifact name="stylesheets/add_namespace" conf="stylesheets"
