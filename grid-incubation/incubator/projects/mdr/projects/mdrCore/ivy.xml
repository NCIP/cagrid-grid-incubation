			type="xsl" />
		<artifact name="stylesheets/fixtag" conf="stylesheets" type="xsl" />
		<artifact name="stylesheets/MetaThesaurusConcept_construct"
			conf="stylesheets" type="xsl" />
		<artifact name="stylesheets/MetaThesaurusConcept_query" conf="stylesheets"
			type="xsl" />
