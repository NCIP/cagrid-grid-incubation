			type="xsl" />
		<artifact name="stylesheets/cadsr_query" conf="stylesheets"
			type="xsl" />
		<artifact name="stylesheets/cadsr_transform" conf="stylesheets"
			type="xsl" />
		<artifact name="stylesheets/openMDR_Classification_Tree" conf="stylesheets"
			type="xsl" />
		<artifact name="stylesheets/openMDR_query" conf="stylesheets"
			type="xsl" />
		<artifact name="stylesheets/openMDR_REST_query" conf="stylesheets"
			type="xsl" />
		<artifact name="stylesheets/openMDR_SKOS_REST_query" conf="stylesheets"
			type="xsl" />
		<artifact name="stylesheets/openMDR_transform" conf="stylesheets"
			type="xsl" />
