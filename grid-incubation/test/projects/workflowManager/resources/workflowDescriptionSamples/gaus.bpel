<?xml version="1.0" encoding="UTF-8" ?>
	<definitions targetNamespace="urn:gauss:gaussService" xmlns="http://schemas.xmlsoap.org/wsdl/" xmlns:add="http://www.mindswap.org/2002/services/add.wsdl" xmlns:multi="http://www.xmethods.net/sd/2001/multi.wsdl" xmlns:div="http://www.xmethods.net/sd/2001/div.wsdl" xmlns:slt="http://schemas.xmlsoap.org/ws/2003/03/partner-link/" xmlns:tns="urn:gauss:gaussService" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
	
	<message name="userRequest">
		<part name="input" type="xsd:string" />
	</message>
	<message name="userResponse">
		<part name="gaussResult" type="xsd:string" />
	</message>
	<portType name="gaussPT">
		<operation name="gauss">
			<input message="tns:userRequest" />
			<output message="tns:userResponse" />
		</operation>
	
	</portType>
	<service name="gaussProcess" />
		<partnerLinks>
			<partnerLink name="User" partnerLinkType="tns:UserSLT" />
			<partnerLink name="addService" partnerLinkType="tns:addSLT" />
			<partnerLink name="multiService" partnerLinkType="tns:multiSLT" />
			<partnerLink name="divService" partnerLinkType="tns:divSLT" />
		</partnerLinks>

		<slt:partnerLinkType name="UserSLT">
			<slt:role name="Receiver">
				<slt:portType name="tns:gaussPT" />
			</slt:role>
		</slt:partnerLinkType>

		<slt:partnerLinkType name="addSLT">
			<slt:role name="addProvider">
				<slt:portType name="add:AddService" />
			</slt:role>
		</slt:partnerLinkType>
		
		<slt:partnerLinkType name="multiSLT">
			<slt:role name="multiProvider">
				<slt:portType name="multi:MultiplyService" />
			</slt:role>
		</slt:partnerLinkType>

		<slt:partnerLinkType name="divSLT">
			<slt:role name="divProvider">
				<slt:portType name="div:DivideService" />
			</slt:role>
		</slt:partnerLinkType>

	</definitions>
