<ns1:CQLQuery xmlns:ns1="http://CQL.caBIG/1/gov.nih.nci.cagrid.CQLQuery">
 <ns1:Target name="gov.nih.nci.cacoresdk.domain.inheritance.childwithassociation.Credit">
  <ns1:Group logicRelation="AND">
   <ns1:Association name="gov.nih.nci.cacoresdk.domain.inheritance.childwithassociation.Bank">
    <ns1:Attribute name="name" predicate="EQUAL_TO" value="Bank3"/>
   </ns1:Association>
   <ns1:Group logicRelation="AND">
    <ns1:Attribute name="amount" predicate="EQUAL_TO" value="3"/>
    <ns1:Attribute name="cardNumber" predicate="EQUAL_TO" value="3"/>
   </ns1:Group>
  </ns1:Group>
 </ns1:Target>
</ns1:CQLQuery>
